{
    "version": 2,
    "builds": [
      {
        "src": "smartscout/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.8" }
      },
      {
        "src": "staticfiles/",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/media/(.*)",
        "dest": "/media/$1"
      },
      {
        "src": "/(.*)",
        "dest": "smartscout/wsgi.py"
      }
    ]
    
  }